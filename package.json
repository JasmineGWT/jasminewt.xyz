{
  "name": "jasminewt.xyz",
  "version": "1.0.0",
  "description": "its dope bruh",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run watch:sass\" \"npm run serve:client\"",
    "test": "browserify test/index.js | node | tap-spec",
    "watch:sass": "sass --watch sass/style.sass:index.css",
    "serve:client": "budo . -d --serve bundle.js --live",
    "build:js": "browserify . -o bundle.js -g uglifyify",
    "build:css": "sass --update sass/style.sass:index.css",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build:js && npm run build:css && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasmineGWT/jasminewt.xyz.git"
  },
  "author": "JasmineGWT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JasmineGWT/jasminewt.xyz/issues"
  },
  "homepage": "https://github.com/JasmineGWT/jasminewt.xyz#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "concurrently": "^3.1.0",
    "get-form-data": "^1.2.5",
    "gh-pages": "^0.11.0",
    "hyperscript": "^2.0.2",
    "morphdom": "^2.2.1",
    "superagent": "^2.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
